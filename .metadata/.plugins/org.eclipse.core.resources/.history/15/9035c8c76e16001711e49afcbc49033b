package l1;

import java.util.Vector;

public class Vertice {
	
	int id, grau;
	Vector<Integer> arestas;
	
	
	
	public Vertice(int id) {
		this.id = id;
		arestas = new Vector<Integer>();
		this.grau = 1;
	}
	

	public void addAresta(int id) {
		arestas.add(id);
	}
	
	public void addGrau() {
		this.grau++;
	}
	

}
